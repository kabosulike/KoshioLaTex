@article{ASASHIBA2020119,
  title    = {Simplicial complexes and tilting theory for {B}rauer tree algebras},
  journal  = {Journal of Algebra},
  volume   = {551},
  pages    = {119 - 153},
  year     = {2020},
  issn     = {0021-8693},
  doi      = {https://doi.org/10.1016/j.jalgebra.2019.12.020},
  url      = {http://www.sciencedirect.com/science/article/pii/S0021869320300284},
  author   = {Hideto Asashiba and Yuya Mizuno and Ken Nakashima},
  keywords = {Brauer tree algebras, 2-term tilting complexes, Simplicial complexes, Derived invariants},
  abstract = {We study 2-term tilting complexes of Brauer tree algebras in terms of simplicial complexes. We show the symmetry and convexity of the lattice polytope corresponding to the simplicial complex of 2-term tilting complexes. Via a geometric interpretation of derived equivalences, we show that the f-vector of the simplicial complexes of Brauer tree algebras only depends on the number of the edges of the Brauer trees and hence it is a derived invariant. In particular, this result implies that the number of 2-term tilting complexes, which is in bijection with support τ-tilting modules, is a derived invariant. Moreover, we apply our result to the enumeration problem of Coxeter-biCatalan combinatorics.}
}
